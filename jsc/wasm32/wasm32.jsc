	    return ThinThin.fstat.call(this, fdno, statbufptr);
	}
	if (this.pwd && path[0] !== "/")
	    path = this.pwd + "/" + path;

        if (!dd)
            return -%{EBADF};

        if (path.length === 0) {
	    let fd = dd;
            let off;
            for (off = 0; off < %{sizeof(struct stat)}; off += 4)
