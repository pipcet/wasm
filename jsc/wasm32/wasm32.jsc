Wasm32System.prototype.step = function ()
{
    if (this.runqueue.length == 0)
        return true;

    let thread = this.runqueue.shift();

    thread.step();

    if (!thread.stopped()) {
	if (this.runqueue.indexOf(thread) === -1) {
            this.runqueue.push(thread);
	}
    }
};

