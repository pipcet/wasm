        let path = this.c_str(pathstr);
        let dd;
	if (path[0] === "/") {
	    dd = this.process.ddroot;
	} else if (path === "") {
	    dd = this.process.ddcwd;
	} else if (fdno >= 0)
	    dd = this.fds[fdno];
	else if (fdno === %{AT_FDROOTD})
	    dd = this.process.ddroot;
	else if (fdno === %{AT_FDCWD} || fdno === -100) /* XXX */
